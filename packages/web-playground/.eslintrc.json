{
  "env": {
    "browser": true
  },
  "extends": [
    "../../.eslintrc.json",
    "plugin:react/recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:jsx-a11y/strict"
  ],
  "plugins": ["react", "jsx-a11y"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "react-hooks/exhaustive-deps": 1
  }
}
